{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "import/extensions": ["error", "ignorePackages"],
    "import/no-unresolved": "off"
  }
}
